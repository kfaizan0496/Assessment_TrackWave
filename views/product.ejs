<link rel="stylesheet" href="/css/product.css">
<%Newsdata.forEach((data)=>{ %>
  <div id="card-container">
  
    <div class="card" style="width: 18rem; margin: 5px;">
      <div class="card-body">
        <img src="<%=data.image%>" class="card-img-top" width="100px" height="100px" alt="News Image">
        <h5 class="card-title"><%=data.title%></h5>
        <h6 class="card-subtitle mb-2 text-body-secondary"><%=data.id%></h6>
        <p class="card-text"><%=data.description%></p>
        <a href="<%=data.url%>" class="card-link" style="text-decoration: none;">News link</a>
      
      </div>
    </div>
  
  </div>

  <%})%>


